    # This option is not active
    # The parent CYGHWR_HAL_CORTEXM_STM32_SPIETH_CONTROLLER is not active
    # ActiveIf constraint: CYGINT_IO_ETH_INT_SUPPORT_REQUIRED
    #     CYGINT_IO_ETH_INT_SUPPORT_REQUIRED (unknown) == 0
