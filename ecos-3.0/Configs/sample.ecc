    # This option is not active
    # The parent CYGHWR_HAL_CORTEXM_STM32_SPIETH_CONTROLLER is not active

    # Flavor: data
    # No user value, uncomment the following line to provide one.
    # user_value 0
    # value_source default
    # Default value: 0
    # Legal values: 0 to 99
};

