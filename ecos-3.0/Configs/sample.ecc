    # value_source default
    # Default value: 0x000000E0
    # Legal values:  0xe0 0xd0 0xc0 0xb0 0xa0 0x90 0x80
    #                                 0x70 0x60 0x50 0x40 0x30 0x20 0x10 0x00
};

